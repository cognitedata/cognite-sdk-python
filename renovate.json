{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>cognitedata/renovate-config-public",
    ":automergeMinor"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchManagers": ["poetry"],
      "matchPackagePatterns": ["*"]
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "labels": [
    "dependencies",
    "auto-merge"
  ]
}
